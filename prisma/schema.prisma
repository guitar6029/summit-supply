generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model hikingboots {
  id         Int       @id @default(autoincrement())
  model      String
  price      Decimal   @db.Decimal(10, 2)
  quantity   Int
  description String
  onSale      Boolean
  size        String[]
  created_at DateTime? @default(now()) @db.Timestamp(6)
}
